{
  "manifest_version": 2,
  "name":"Datastar Inspector",
  "version": "0.1",
  "description":"Inspect state changes and events in datastar apps",
  "icons":{
    "48": "./rocket.png"
  },
  "permissions":["http://localhost/*"],
  "background": {
      "scripts":["background.js"],
      "persistent": false
  },
  "content_scripts": [{
      "js": ["./content_script.js"],
      "matches":["http://localhost/*"]
  }],
  "devtools_page": "./devtool.html"
}
